{
  "manifest_version": 2,

  "name": "A New 'new tab'",
  "version": "7.0.1",
  "chrome_url_overrides": {
    "newtab": "/main.html"
  },
  "options_page": "/settings.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "file:///*"],
  "optional_permissions": ["https://*/"]
}
