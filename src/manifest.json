{
  "manifest_version": 2,

  "name": "A New 'new tab'",
  "version": "3.4.2",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "options_page": "secondary.html",
  "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "file:///*"]
}
