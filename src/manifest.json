{
  "manifest_version": 2,

  "name": "Custom New Tab",
  "version": "7.1.4",
  "chrome_url_overrides": {
    "newtab": "/main.html"
  },
  "options_page": "/settings.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "file:///*"],
  "optional_permissions": ["https://*/"],
  "icons": {
    "18": "extension-icons/ic_tab_black_18dp.png",
    "24": "extension-icons/ic_tab_black_24dp.png",
    "36": "extension-icons/ic_tab_black_36dp.png",
    "48": "extension-icons/ic_tab_black_48dp.png"
  }
}
